# Map movement controls to the left stick. The same can be done using 'right' instead of 'left'.

enter = L1
q = L2

c = L3
n = R3

w = leftY-
a = leftX-
s = leftY+
d = leftX+

up = dpadUp
left = dpadLeft
right = dpadRight
down = dpadDown

t = PS
1 = share
0 = options

u = R1
o = R2

i = rightY-
j = rightX-
k = rightY+
l = rightX+

f = square
space = X
e = triangle
# shift = O
h = O

m = touchpad
